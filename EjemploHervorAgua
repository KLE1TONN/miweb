package Practicas;

import java.util.Scanner;

public class EjemploHervorAgua {
    public static void main(String[] args) {
        Scanner tcl = new Scanner (System.in);
        int hierve = 0;
        hierve = tcl.nextInt();
        if (hierve <= 100) {
            System.out.println("EL AGUA NO HIERVE");
        } else if (hierve >= 1000){
            System.out.println("EL AGUA HIRVIO");
        } else { 
                    System.out.println("EL AGUA HIERVE");
        }
        
    }
    
}
